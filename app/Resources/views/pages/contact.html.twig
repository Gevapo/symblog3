{% extends 'layout.html.twig' %}

{% block title %}Contact{% endblock%}

{% block body %}
    <header>
        <h1>Blog contact page</h1>
    </header>

    <p>Want to contact symblog?</p>

    {#
    {{ form_start(form) }}
    #}
    <form action="{{ path('contact') }}" method="post" class="blogger" enctype="multipart/form-data">
    {{ form_errors(form) }}

    {{ form_row(form.name) }}
    {{ form_row(form.email) }}
    {{ form_row(form.subject) }}
    {{ form_row(form.body) }}

    <input type="submit" value="Submit" />

    {{ form_end(form) }}
{% endblock %}